{
  "name": "zabachok/symfobooster-devkit",
  "type": "symfony-bundle",
  "description": "Tools for making awesome code using symfobooster",
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniil Romanov",
      "email": "zabachok@zabachok.net"
    }
  ],
  "require": {
    "php": ">=8.1",
    "zabachok/symfobooster": "@dev",
    "symfony/yaml": "^6.1",
    "symfony/validator": "^6.1",
    "symfony/serializer": "^6.1",
    "symfony/property-access": "^6.1",
    "symfony/http-foundation": "^6.1",
    "nette/php-generator": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "10.1",
    "symfony/browser-kit": "6.1.*"
  },
  "autoload": {
    "psr-4": {
      "Zabachok\\SymfoboosterDevkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zabachok\\SymfoboosterDevkit\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
