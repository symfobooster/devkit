{
  "name": "symfobooster/devkit",
  "type": "symfony-bundle",
  "description": "Tools for making awesome code using symfobooster",
  "minimum-stability": "stable",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniil Romanov",
      "email": "symfobooster@zabachok.net"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/yaml": "6.3.*",
    "symfony/validator": "6.3.*",
    "symfony/serializer": "6.3.*",
    "symfony/property-access": "6.3.*",
    "symfony/http-foundation": "6.3.*",
    "nette/php-generator": "^4.0",
    "phpunit/phpunit": "10.2.*",
    "symfony/browser-kit": "6.3.*",
    "squizlabs/php_codesniffer": "^3.7",
    "phpdocumentor/reflection-docblock": "^5.3.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Symfobooster\\Devkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symfobooster\\Devkit\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "lint": "./vendor/bin/phpcs -p -n --colors"
  }
}
