{
  "name": "zabachok/symfobooster-devkit",
  "type": "symfony-bundle",
  "description": "Tools for making awesome code using symfobooster",
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniil Romanov",
      "email": "zabachok@zabachok.net"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "zabachok/symfobooster": "@dev",
    "symfony/yaml": "^6.0",
    "symfony/validator": "^6.0",
    "symfony/serializer": "^6.0",
    "symfony/property-access": "^6.0",
    "symfony/http-foundation": "^6.0",
    "nette/php-generator": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Zabachok\\SymfoboosterDevkit\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
